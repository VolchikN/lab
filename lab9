#include <iostream>

using namespace std;

int main()
{
    setlocale(LC_ALL, "ru");
   cout << "\n Зад 1" << endl;
    int N, n;
    cout << "Введите количество секунд N, прошедших с начала суток:";
    cin >> N;
    n = N% 60;
    cout << "Количество секунд n, прошедших с начала последней минуты: " << n << endl;
    
    
    cout << "\n Зад 2" << endl;
    int K;
    cout << "Введите число К в диапазоне 1-365:";
    cin >> K;
    n= K%7;
    cout << "Номер дня недели для К-го дня года: " << n << endl;
    
    
    cout << "\n Зад 3" << endl;
    cout << "Введите число К в диапазоне 1-365:";
    cin >> K;
    cout << "Введите число N в диапазоне 1-7:";
    cin >> N;
    n= (N + K -2)%7 +1;
    cout << "Номер дня недели для К-го дня года: " << n << endl; 
    
    
     cout << "\n Зад 4" << endl;
     int A, B, C, S, S1, s;
      cout << "Введите целое число А:";
    cin >> A;
     cout << "Введите целое число В:";
    cin >> B;
     cout << "Введите целое число С:";
    cin >> C;
    S= A*B;
    S1= C*C;
    n=(A/C)*(B/C);
    s= S-S1*n;
    cout << "Количество квадратов, которое можно разместить в прямоугольнике: " << n << endl; 
    cout << "Площадь незанятой части прямоугольника: " << s << endl; 
    

    cout << "\n Зад 5" << endl;
    int G, st;
    cout << "Введите год:";
    cin >> G;
    st = (G-1)/100 + 1;
    cout << "Столетие: " << st << endl;
    return 0;
}
